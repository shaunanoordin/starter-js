  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.1",
    "express": "^4.17.1",
    "sass": "^1.32.8",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0"
  },
  "engines": {
    "node": ">=14"
  }
}
